<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dimo.academic_affairs_system.mapper.StudentMapper">
    <delete id="delete_student">
        delete from relation.stc where student_id = #{studentId};
        delete from user.student where id = #{studentId};
    </delete>
</mapper>